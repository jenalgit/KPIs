{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/skarampatakis/easyrdf"
        },
        {
            "type": "vcs",
            "url": "https://github.com/larjohn/asparagus"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "appzcoder/crud-generator": "^2.0",
        "barryvdh/laravel-debugbar": "^2.3",
        "barryvdh/laravel-ide-helper": "^2.2",
        "benestar/asparagus": "dev-obeuviz",
        "dimsav/laravel-translatable": "^7.0",
        "doctrine/dbal": "^2.5",
        "easyrdf/easyrdf": "dev-okgreece",
        "fx3costa/laravelchartjs": "^2.2",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "ml/json-ld": "^1.0",
        "propaganistas/laravel-translatable-bootforms": "~1.3",
        "semsol/arc2": "^2.3",
        "vluzrmos/language-detector": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
